---
title:
---

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <title>
            {{ title | safe }}
            {% if title %}&rsaquo;{% endif %}
            {{ site.name }}
        </title>

        <meta name="theme-color" content="hsl(240, 13%, 98%)">
        <meta name="view-transition" content="same-origin">

        <link rel="preload" href="/fonts/lexend-deca.woff2" as="font" type="font/woff2" crossorigin>

        <link rel="stylesheet" href="/dist/msme.min.css">
        <link rel="stylesheet" href="/dist/prism.min.css">

        {% include 'layout/head-meta.njk' %}
    </head>
    <body>
        <a href="#main" class="visually-hidden skiplink">
            Skip to content.
        </a>
        
        <div class="page-wrapper">
            {% include "layout/header.njk" %}

            <main id="main" tabindex="-1">
                {{ content | safe }}
            </main>
        </div>

        <div class="footer-wrapper">
            {% include "layout/footer.njk" %}
        </div>

        <script>
            {% set serviceworkerEmbed %}
                {% include 'website/js/_serviceworker-embed.js' %}
            {% endset %}
            {{ serviceworkerEmbed | safe }}
        </script>

        <script src="/instantpage.js" async></script>

        {% include "jsonld/website.njk" %}
    </body>
</html>
